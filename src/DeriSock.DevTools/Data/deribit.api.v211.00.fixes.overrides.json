{
  "methods": {
    ".+": {
      "response": {
        "properties": {
          "acccess_token": {
            "name": "access_token"
          },
          ".+": {
            "properties": {
              "session_bankrupcy": {
                "name": "session_bankruptcy"
              }
            }
          }
        }
      }
    },
    "^public/(get_order_book|get_order_book_by_instrument_id|ticker)$": {
      "response": {
        "properties": {
          "underlying_index": {
            "apiDataType": "string",
            "dataType": "string"
          }
        }
      }
    },
    "private/get_user_trades_by_order": {
      "response": {
        "apiDataType": "array of objects",
        "dataType": "array",
        "arrayDataType": "UserTrade"
      }
    },
    "private/create_combo": {
      "request": {
        "properties": {
          "trades": {
            "properties": {
              "amount": {
                "apiDataType": "integer",
                "dataType": "integer"
              }
            }
          }
        }
      }
    }
  },
  "subscriptions": {
    "book.{instrument_name}.{interval}": {
      "request": {
        "properties": {
          "interval": {
            "enumValues": [
              "100ms",
              "raw"
            ]
          }
        }
      }
    },
    "incremental_ticker.{instrument_name}": {
      "response": {
        "properties": {
          "underlying_index": {
            "apiDataType": "string",
            "dataType": "string"
          }
        }
      }
    },
    "ticker.{instrument_name}.{interval}": {
      "response": {
        "properties": {
          "underlying_index": {
            "apiDataType": "string",
            "dataType": "string"
          }
        }
      }
    },
    "trades.{instrument_name}.{interval}": {
      "response": {
        "apiDataType": "array of objects",
        "dataType": "array",
        "arrayDataType": "PublicTrade"
      }
    },
    "user.changes.{instrument_name}.{interval}": {
      "response": {
        "apiDataType": "object",
        "dataType": "UserChange"
      }
    },
    "user.changes.{kind}.{currency}.{interval}": {
      "response": {
        "apiDataType": "object",
        "dataType": "UserChange"
      }
    }
  }
}